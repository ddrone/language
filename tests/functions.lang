fun fib(x: int): int {
    if (x == 0 || x == 1) {
        return x;
    }
    return fib(x - 1) + fib(x - 2);
}

fun factorial(x: int): int {
    if (x == 0 || x == 1) {
        return 1;
    }
    return debug x * factorial(x - 1);
}

