fun factorial(x: int): int {
    if (x <= 1) {
        return 1;
    }
    return x * factorial(x - 1);
}

fun stepwise(v: vm<int>): int {
    if (step(v)) {
        return extract(v);
    } else {
        debug stack(v);
        return stepwise(v);
    }
}

